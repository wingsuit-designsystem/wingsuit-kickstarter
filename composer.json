{
    "name": "wingsuit-designsystem/wingsuit-kickstarter",
    "type": "project",
    "description": "Starterkit for Wingsuit and Layout Builder.",
    "license": "GPL-2.0-or-later",
    "repositories": {
        "lenient": {
            "type": "composer",
            "url": "https://packages.drupal.org/lenient"
        },
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"

        },
        "assets": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }

    },
    "require": {
        "mglaman/composer-drupal-lenient": "^1.0",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.6.0",
        "oomphinc/composer-installers-extender": "^2.0",
        "acquia/blt": "*",
        "ext-dom": "*",
        "drupal/core-composer-scaffold": "^9.4",
        "drupal/core-project-message": "^9.4",
        "drupal/core-recommended": "^10.0",
        "drupal/core": "~10.0",
        "drupal/config_split": "^1.5",
        "drupal/components": "^3.0",
        "drupal/ui_patterns_layout_builder": "^1.2",
        "drupal/ui_patterns_field_formatters": "2.x-dev",
        "drupal/component_blocks": "^1.2",
        "drupal/focal_point": "^2.0@alpha",
        "drupal/media_library_edit": "^3.0",
        "drupal/media_responsive_thumbnail": "^1.2",
        "drupal/gin": "^3.0@alpha",
        "drupal/gin_lb": "~1.0",
        "drupal/admin_toolbar": "^3.0",
        "drupal/inline_block_title_automatic": "^1.0@alpha",
        "drupal/ui_patterns_extends": "^1.0@beta",
        "drupal/twig_tweak": "^3.0",
        "drupal/layout_builder_st": "^1.0@alpha",
        "drupal/field_group": "^3.1",
        "drupal/easy_responsive_images": "^1.0",
        "drupal/webform": "^6.0",
        "drupal/link_attributes": "^1.11",
        "drupal/google_tag": "^1.4",
        "drupal/schema_metatag": "^2.3",
        "drupal/autosave_form": "^1.2",
        "drupal/conflict": "~2.0",
        "drupal/ctools": "^3.5",
        "drupal/diff": "^1.0",
        "drupal/dropzonejs": "^2.5",
        "drupal/entity_block": "^1.0@beta",
        "drupal/image_widget_crop": "^2.3",
        "drupal/layout_builder_restrictions": "^2.8",
        "drupal/redirect": "^1.6",
        "drupal/schemata": "^1.0@beta",
        "drupal/simple_gmap": "^3.0",
        "drupal/pathauto": "^1.8",
        "drupal/linkit": "~6.0",
        "drupal/editor_advanced_link": "^2.0",
        "drupal/twig_vardumper": "^3.1",
        "drupal/media_library_form_element": "^2.0",
        "drupal/section_library": "^1.0",
        "drupal/ui_patterns_settings": "2.x-dev",
        "bower-asset/dropzone": "5.7.2",
        "drupal/section_library_reusable": "^1.0@beta",
        "drupal/svg_image_field": "^2.1",
        "drupal/viewsreference": "^2.0",
        "drupal/wingsuit_companion": "~2.0",
        "drupal/imageapi_optimize_webp": "^2.0@beta",
        "drupal/media_bulk_upload": "^3.0",
        "drupal/devel": "^5.1",
        "drupal/bg_image_formatter": "^1.16",
        "drupal/colorwidget": "1.0.x-dev",
        "drupal/stable": "^2.0",
        "drupal/toolbar_anti_flicker": "^9.3",
        "drupal/metatag_open_graph": "^1.22",
        "drupal/formdazzle": "^3.0",
        "drupal/webform_config_ignore": "^1.3",
        "drupal/advanced_text_formatter": "^3.0@RC",
        "drupal/eu_cookie_compliance": "^1.24",
        "drupal/easy_breadcrumb": "^2.0"
    },
    "require-dev": {
        "acquia/blt-phpcs": "^1.0"
    },
    "config": {
        "process-timeout": 0,
        "allow-plugins": {
            "mglaman/composer-drupal-lenient": true,
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "acquia/blt": true,
            "acquia/blt-phpcs": true,
            "drupal/core-project-message": true,
            "oomphinc/composer-installers-extender": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "patches": {
            "drupal/component_blocks": {
                "3210538 - Not working with Settings Type url and token ": "https://git.drupalcode.org/project/component_blocks/-/merge_requests/2.patch"
            },
            "drupal/layout_builder_browser": {
                "Add option for node local tasks in toolbar.": "https://www.drupal.org/files/issues/2023-03-17/layout_builder_browser-d10-compatibility-3297496-14.patch"
            },
            "drupal/imageapi_optimize_webp": {
                "Automated Drupal 10 compatibility fixes": "https://www.drupal.org/files/issues/2022-12-29/imageapi_optimize_webp.2.x-dev.d10compatibility.patch"
            },
            "drupal/core": {
                "Consider supporting Layout Builder Overrides for other view modes": "./patches/test.patch",
                "Improve StringItem::generateSampleValue()": "https://www.drupal.org/files/issues/2020-06-24/3091478-15.patch",
                "Not all form elements can't use in vertical_tabs": "https://www.drupal.org/files/issues/2022-03-14/2190333-20.patch",
                "PHP message: Error: Call to a member function getEntityTypeId() on null (Layout Builder)": "https://www.drupal.org/files/issues/2020-08-05/3049332-inline-35.patch",
                "Stream wrapper reference in JS library causes error in _locale_parse_js_file()": "https://www.drupal.org/files/issues/2020-05-08/2735717-28.patch",
                "New non translatable field on translatable content throws error": "https://www.drupal.org/files/issues/2021-02-10/3025039_42.patch",
                "2869592 - Disabled update module shouldn't produce a status report warning": "https://www.drupal.org/files/issues/2020-02-07/2869592-remove-update-warning-34.patch",
                "Layout builder doesn't support bundle computed field": "https://www.drupal.org/files/issues/2019-02-22/layoutbuilder-bundlecomputedfieldsfix-3034979-3-8.6x.patch",
                "Workaround for \"Call to a member function getLabel() after enabling layout_builder\"": "https://www.drupal.org/files/issues/2020-04-08/2985882-field-85.patch",
                "Installing a module causes translations to be overwritten": "https://www.drupal.org/files/issues/2021-08-10/2806009-disable-locale-config-sync-65.patch"
            },
            "drupal/layout_builder_st": {
                "Argument 1 passed to Drupal\\layout_builder_st\\EventSubscriber\\ComponentPluginTranslate::isTranslation() must implement interface Drupal\\layout_builder\\SectionStorageInterface, null given": "https://www.drupal.org/files/issues/2019-07-24/isTranslation-null-given.patch",
                "Inline block loading active block_content revision even if serialized block exists\n": "https://www.drupal.org/files/issues/2020-05-15/layout_builder_st-3067646-10.patch"
            },
            "drupal/section_library": {
                "#3228163: Add create or update logic.": "https://www.drupal.org/files/issues/2022-12-09/section-library-3228163-10-refactored.patch"
            },
            "drupal/ui_patterns": {
                "Allow for Layout Builder sections to be given administrativ.": "https://patch-diff.githubusercontent.com/raw/nuvoleweb/ui_patterns/pull/278.patch",
                "The \"use\" attribute should also work for variants": "https://git.drupalcode.org/project/ui_patterns/-/merge_requests/21.patch"
            },
            "drupal/ui_patterns_field_formatters": {
                "Add item to context.": "https://git.drupalcode.org/project/ui_patterns_field_formatters/-/merge_requests/18.patch"
            },
            "drupal/conflict": {
                "PHP error when stored entity is not found": "https://www.drupal.org/files/issues/2020-08-05/3163662-2.patch"
            },
            "drupal/media_bulk_upload": {
                "Drupal 10 compatibility": "https://www.drupal.org/files/issues/2023-01-16/3333989_media_bulk_upload_2.patch"
            },
            "drupal/toolbar_anti_flicker": {
                "Drupal 10 compatibility": "https://www.drupal.org/files/issues/2022-07-23/toolbar_anti_flicker.9.3.x-dev.rector.patch"
            },
            "drupal/colorwidget": {
                "Drupal 10 compatibility": "https://www.drupal.org/files/issues/2022-06-15/colorwidget.1.0.x-dev.rector.patch"
            }
        },
        "composer-exit-on-patch-failure": true,
        "drupal-scaffold": {
            "locations": {
                "web-root": "docroot/"
            }
        },
        "enable-patching": true,
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "docroot/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "docroot/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "installer-types": [
            "drupal-profile",
            "drupal-module",
            "drupal-core",
            "drupal-library",
            "bower-asset",
            "npm-asset"
        ],
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "drupal-lenient": {
            "allowed-list": ["drupal/layout_builder_browser", "drupal/colorwidget", "drupal/imageapi_optimize_webp", "drupal/media_bulk_upload", "drupal/colorwidget", "drupal/toolbar_anti_flicker"]
        }
    }
}
